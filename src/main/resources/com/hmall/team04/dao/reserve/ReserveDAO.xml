<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace ="com.hmall.team04.dao.reserve.ReserveDAO">

<select id = "getReservebyUserId" resultType = "int">
		select
			reserve
		from user_reserve_t
		where user_id = #{userid}
</select>


</mapper>